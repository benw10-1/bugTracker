<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css'
    />
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css'
      integrity='sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3'
      crossorigin='anonymous'
    />
    <link href='/style/bugs.css' rel='stylesheet' />

    <title>Document</title>
  </head>
  <body class='bug-body'>
    <nav class='navbar'>
      <div id='main-logo'><i class='fa-solid fa-bug-slash'></i>Bug Tracker</div>
    </nav>
    <div class='back-container'>
      <h1>
        <a href='./dashboard' id='login-back'> <strong>&#60;</strong> Back</a>
      </h1>
    </div>
    <main class='main-bug-container'>
      <div class='project-name-container'>
        <h6>{project's name}</h6>
      </div>
      <div class='contributors-container'>
        <div class='contributors-header'>
          <h6>Contributors</h6>
          <button class='add-contributor-btn'><a href='/projects/create'></a>Add
            member</button>
        </div>
        <div class='table-contributors'>
          <table class='table'>
            <thead class='thead-dark'>
              <tr>
                <th scope='col'>NAME</th>
                <th scope='col'>EMAIL</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>hello</td>
                <td>hello</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
      <div class='bugs-container'>
        <div class='bugs-header'>
          <h6>Bugs/Issues</h6>
          <button class='add-bug-btn'><a href='/projects/create'></a>Add Bug</button>
        </div>
        <div class='table-bug'>
          <table class='table'>
            <thead class='thead-dark'>
              <tr>
                <th scope='col'>BUG TITLE</th>
                <th scope='col'>DESCRIPTION</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>hello</td>
                <td>hello</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>

    </main>

  </body>
</html>